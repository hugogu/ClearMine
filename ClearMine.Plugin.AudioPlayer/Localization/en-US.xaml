<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <s:String x:Key="AP_Name">Music player</s:String>
    <s:String x:Key="AP_Description">Provide sound effect to some key game event.</s:String>
    <s:String x:Key="AP_VolumnName">Volumn</s:String>
    <s:String x:Key="AP_VolumnDescription">Set the volumn of the sound</s:String>
    <s:String x:Key="AP_WonMusic">Won music</s:String>
    <s:String x:Key="AP_WonMusicDescription">The music need to play when won.</s:String>
    <s:String x:Key="AP_LostMusic">Lost music</s:String>
    <s:String x:Key="AP_LostMusicDescription">The music need to play when lost.</s:String>
    <s:String x:Key="AP_NewGameMusic">New game music</s:String>
    <s:String x:Key="AP_NewGameMusicDescription">The music need to play when start a new game.</s:String>
    <s:String x:Key="AP_ExpandMultipleMusic">Expand multiple music</s:String>
    <s:String x:Key="AP_ExpandMultipleDescription">The music need to play when multiple cells expanded.</s:String>
    <s:String x:Key="AP_ExpandSingleMusic">Expand single music</s:String>
    <s:String x:Key="AP_ExpandSingleDescription">The music need to play when one cell expanded.</s:String>
</ResourceDictionary>